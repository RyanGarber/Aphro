{
  "name": "@ryangarber/aphro",
  "version": "0.1.0",
  "description": "A useful Discord bot.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "node build.js",
    "build:watch": "node build.js --watch",
    "clean": "rm -rf ./dist",
    "prebuild": "npm run lint",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .fs --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "dev": "node --import tsx --watch --enable-source-maps src/index.ts",
    "start": "node dist/index.js",
    "test": "echo \"NYI\" && exit 0",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "typescript-eslint": "^8.52.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^25.0.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
